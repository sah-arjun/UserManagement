{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "import/prefer-default-export": "off",
        "jsx-boolean-value": "off",
        "object-shorthand": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": [
            "enabled",
            { "ignore": "ignore", "customValidators": "customValidator" }
          ],
          "quote-props": "off",
          "no-console": "off"
          
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }

}